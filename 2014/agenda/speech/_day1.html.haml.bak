.time-block 09:15
.row
  %section.session.ceremony
    .header-info
      %span
        主題演講一
      場次主持：陳銘憲 (中央研究院資訊科技創新研究中心)
    %h3.title
      Big-data analytics: challenges and opportunities
    .info
      - speaker = speaker_file("cjlin");
      .speaker-in-speech{:"data-name"=>"#{speaker['tags'].join(",")}"}
        .speaker-block
          .mask
          .speaker-name= speaker['name']
          - speaker['title'].each do |title|
            .speaker-title= title
          - speaker['sites'].each do |site|
            %a.speaker-info{:href=>"#{site['href']}"}= site['title']
          .speaker-bio= speaker['bio']
        %img{:src=>"/images/#{speaker['img']}"}
      %time 09:15-10:20 (65 mins)

