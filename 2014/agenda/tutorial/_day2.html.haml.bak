.time-block 08:30
%section.session.room-rest
  %h3.title 報到
  .info
    %time 08:30-09:15 (45 mins)

.time-block 09:15
.row
  %section.session.ceremony
    %h3.title 資料科學在 Whoscall 產品體系中的角色
    .info
      - speaker = speaker_file("jeff");
      .speaker-in-speech{:"data-name"=>"#{speaker['tags'].join(",")}"}
        .speaker-block
          .mask
          .speaker-name= speaker['name']
          - speaker['title'].each do |title|
            .speaker-title= title
          - speaker['sites'].each do |site|
            %a.speaker-info{:href=>"#{site['href']}"}= site['title']
          .speaker-bio= speaker['bio']
        %img{:src=>"/images/#{speaker['img']}"}
      - speaker = speaker_file("yimin");
      .speaker-in-speech{:"data-name"=>"#{speaker['tags'].join(",")}"}
        .speaker-block
          .mask
          .speaker-name= speaker['name']
